<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="GestureView">
        <attr name="gest_movementAreaWidth" format="reference|dimension" />
        <attr name="gest_movementAreaHeight" format="reference|dimension" />
        <attr name="gest_minZoom" format="reference|float" />
        <attr name="gest_maxZoom" format="reference|float" />
        <attr name="gest_doubleTapZoom" format="reference|float" />
        <attr name="gest_overzoomFactor" format="reference|float" />
        <attr name="gest_overscrollX" format="reference|dimension" />
        <attr name="gest_overscrollY" format="reference|dimension" />
        <attr name="gest_fillViewport" format="reference|boolean" />
        <attr name="gest_gravity">
            <!-- Values are copied from system "gravity" attribute -->
            <flag name="top" value="0x30" />
            <flag name="bottom" value="0x50" />
            <flag name="left" value="0x03" />
            <flag name="right" value="0x05" />
            <flag name="center_vertical" value="0x10" />
            <flag name="center_horizontal" value="0x01" />
            <flag name="center" value="0x11" />
            <flag name="start" value="0x00800003" />
            <flag name="end" value="0x00800005" />
        </attr>
        <attr name="gest_fitMethod">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
            <enum name="inside" value="2" />
            <enum name="outside" value="3" />
            <enum name="none" value="4" />
        </attr>
        <attr name="gest_boundsType">
            <enum name="normal" value="0" />
            <enum name="inside" value="1" />
            <enum name="outside" value="2" />
            <enum name="pivot" value="3" />
            <enum name="none" value="4" />
        </attr>
        <attr name="gest_panEnabled" format="reference|boolean" />
        <attr name="gest_flingEnabled" format="reference|boolean" />
        <attr name="gest_zoomEnabled" format="reference|boolean" />
        <attr name="gest_rotationEnabled" format="reference|boolean" />
        <attr name="gest_restrictRotation" format="reference|boolean" />
        <attr name="gest_doubleTapEnabled" format="reference|boolean" />
        <attr name="gest_exitEnabled" format="reference|boolean" />
        <attr name="gest_animationDuration" format="reference|integer" />
        <attr name="gest_disableGestures" format="reference|boolean" />
        <attr name="gest_disableBounds" format="reference|boolean" />
    </declare-styleable>

    <!-- Added copy with GestureImageView name to enable autocomplete in AS -->
    <declare-styleable name="GestureImageView">
        <attr name="gest_movementAreaWidth" />
        <attr name="gest_movementAreaHeight" />
        <attr name="gest_minZoom" />
        <attr name="gest_maxZoom" />
        <attr name="gest_doubleTapZoom" />
        <attr name="gest_overzoomFactor" />
        <attr name="gest_overscrollX" />
        <attr name="gest_overscrollY" />
        <attr name="gest_fillViewport" />
        <attr name="gest_gravity" />
        <attr name="gest_fitMethod" />
        <attr name="gest_boundsType" />
        <attr name="gest_panEnabled" />
        <attr name="gest_flingEnabled" />
        <attr name="gest_zoomEnabled" />
        <attr name="gest_rotationEnabled" />
        <attr name="gest_restrictRotation" />
        <attr name="gest_doubleTapEnabled" />
        <attr name="gest_exitEnabled" />
        <attr name="gest_animationDuration" />
        <attr name="gest_disableGestures" />
        <attr name="gest_disableBounds" />
    </declare-styleable>

    <!-- Added copy with GestureFrameLayout name to enable autocomplete in AS -->
    <declare-styleable name="GestureFrameLayout">
        <attr name="gest_movementAreaWidth" />
        <attr name="gest_movementAreaHeight" />
        <attr name="gest_minZoom" />
        <attr name="gest_maxZoom" />
        <attr name="gest_doubleTapZoom" />
        <attr name="gest_overzoomFactor" />
        <attr name="gest_overscrollX" />
        <attr name="gest_overscrollY" />
        <attr name="gest_fillViewport" />
        <attr name="gest_gravity" />
        <attr name="gest_fitMethod" />
        <attr name="gest_boundsType" />
        <attr name="gest_panEnabled" />
        <attr name="gest_flingEnabled" />
        <attr name="gest_zoomEnabled" />
        <attr name="gest_rotationEnabled" />
        <attr name="gest_restrictRotation" />
        <attr name="gest_doubleTapEnabled" />
        <attr name="gest_exitEnabled" />
        <attr name="gest_animationDuration" />
        <attr name="gest_disableGestures" />
        <attr name="gest_disableBounds" />
    </declare-styleable>


    <declare-styleable name="CropAreaView">
        <attr name="gest_backgroundColor" format="reference|color" />
        <attr name="gest_borderColor" format="reference|color" />
        <attr name="gest_borderWidth" format="reference|dimension" />
        <attr name="gest_rulesHorizontal" format="reference|integer" />
        <attr name="gest_rulesVertical" format="reference|integer" />
        <attr name="gest_rulesWidth" format="reference|dimension" />
        <attr name="gest_rounded" format="reference|boolean" />
        <attr name="gest_aspect" format="reference|float" />
    </declare-styleable>
    <declare-styleable name="SlidingUpPanelLayout">
        <attr name="umanoPanelHeight" format="dimension" />
        <attr name="umanoShadowHeight" format="dimension" />
        <attr name="umanoParallaxOffset" format="dimension" />
        <attr name="umanoFadeColor" format="color" />
        <attr name="umanoFlingVelocity" format="integer" />
        <attr name="umanoDragView" format="reference" />
        <attr name="umanoScrollableView" format="reference" />
        <attr name="umanoOverlay" format="boolean"/>
        <attr name="umanoClipPanel" format="boolean"/>
        <attr name="umanoAnchorPoint" format="float" />
        <attr name="umanoInitialState" format="enum">
            <enum name="expanded" value="0" />
            <enum name="collapsed" value="1" />
            <enum name="anchored" value="2" />
            <enum name="hidden" value="3" />
        </attr>
        <attr name="umanoScrollInterpolator" format="reference" />
    </declare-styleable>
////
    <attr name="siArrowPosition">
        <enum name="left" value="0" />
        <enum name="right" value="1" />
    </attr>
    <attr name="siBorderType">
        <enum name="stroke" value="0" />
        <enum name="fill" value="1" />
    </attr>
    <attr name="siStrokeCap">
        <enum name="butt" value="0" />
        <enum name="round" value="1" />
        <enum name="square" value="2" />
    </attr>
    <attr name="siStrokeJoin">
        <enum name="bevel" value="0" />
        <enum name="miter" value="1" />
        <enum name="round" value="2" />
    </attr>
    <declare-styleable name="ShaderImageView">
        <attr name="siSquare" format="boolean" />
        <attr name="siBorderColor" format="color" />
        <attr name="siBorderWidth" format="dimension" />
        <attr name="siBorderAlpha" format="float" />
        <attr name="siForeground" format="integer|reference" />
        <!-- Rounded Image View -->
        <attr name="siRadius" format="dimension" />
        <!--BubbleImageView-->
        <attr name="siArrowPosition"/>
        <attr name="siTriangleHeight" format="dimension"/>
        <!--PorterImageView-->
        <attr name="siShape" format="integer|reference" />
        <!--ShaderImageView-->
        <attr name="siBorderType"/>
        <attr name="siStrokeCap"/>
        <attr name="siStrokeJoin"/>
        <attr name="siStrokeMiter" format="dimension" />
    </declare-styleable>

    ///
    <declare-styleable name="TedBottomPickerImageView">
        <attr name="foreground" format="reference"/>
        <attr name="fit_mode" format="string"/>
    </declare-styleable>



    <declare-styleable name="TedBottomPickerSquareView">
        <attr name="matchHeightToWidth" format="boolean"/>
        <attr name="matchWidthToHeight" format="boolean"/>
    </declare-styleable>

</resources>
